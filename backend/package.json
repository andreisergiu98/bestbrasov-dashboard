	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "node .dist",
		"start:dev": "env FORCE_COLOR=true concurrently -n codegen,app -c magenta,blue \"yarn generate:dev\" \"yarn build:dev\"",
		"build": "yarn generate && tsc && node esbuild.config.js",
