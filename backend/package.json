	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "node ./build/server.js",
		"start:dev": "tsc-watch --onSuccess 'dotenv -- node ./build/server.js'",
		"build": "tsc",
		"lint": "eslint . --ext .js,.ts",
		"prisma": "dotenv -- prisma",
		"generate": "dotenv -- pnpify prisma generate",
		"postbuild": "cp -r ./src/__generated__/prisma ./build/__generated__/prisma",
		"emit-schema": "dotenv -- node ./build/schema-emit.js"
	},
