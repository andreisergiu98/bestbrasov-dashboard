{
	"name": "@bestbrasov/backend",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "node .dist",
		"start:dev": "env FORCE_COLOR=true yarn build && concurrently -n codegen,build,types,app -c magenta,cyan,blue,green 'yarn generate:dev' 'yarn build:dev' 'yarn types:dev' 'yarn start:app:dev'",
		"start:app:dev": "dotenv -- node-dev --inspect -r source-map-support/register .dist/index.js | pino-pretty -c -t SYS:dd.mm.yyyy HH:MM:ss",
		"build": "swcb backend --out-dir backend/.dist --copy-files",
		"build:dev": "swcb backend --quiet --out-dir backend/.dist --copy-files -w",
		"generate": "yarn prisma generate",
		"generate:dev": "yarn prisma generate --watch",
		"types": "tsc --noEmit",
		"types:dev": "tsc --preserveWatchOutput --pretty -w",
		"test": "yarn jest --config .jestrc.js",
		"lint": "eslint . --ext .js,.ts",
		"prisma": "pnpify prisma",
		"migrate": "yarn dlx @yarnpkg/pnpify prisma migrate deploy"
	},
	"bin": {
		"swcb": ".swc.cli.js"
	},
	"dependencies": {
		"@bestbrasov/common": "workspace:packages/common",
		"@koa/cors": "^3.1.0",
		"@koa/router": "^10.1.1",
		"apollo-server-koa": "^3.3.0",
		"bullmq": "^1.48.3",
		"chalk": "^4.1.2",
		"class-validator": "^0.13.1",
		"date-fns": "^2.24.0",
		"eventemitter3": "^4.0.7",
		"googleapis": "^88.2.0",
		"graphql": "^15.6.0",
		"graphql-fields": "^2.0.3",
		"graphql-redis-subscriptions": "^2.4.0",
		"graphql-scalars": "^1.10.1",
		"graphql-subscriptions": "^1.2.1",
		"graphql-ws": "^5.5.0",
		"ioredis": "^4.27.10",
		"jsonwebtoken": "^8.5.1",
		"koa": "^2.13.3",
		"koa-bodyparser": "^4.3.0",
		"openid-client": "^4.9.0",
		"pino": "^7.0.0-rc.8",
		"pretty-bytes": "^5.6.0",
		"pretty-ms": "^7.0.1",
		"reflect-metadata": "^0.1.13",
		"type-graphql": "^1.1.1",
		"ua-parser-js": "^0.7.28",
		"universal-cookie": "^4.0.4",
		"ws": "^8.2.3"
	},
	"devDependencies": {
		"@prisma/client": "3.1.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.93",
		"@swc/jest": "^0.2.4",
		"@types/graphql-fields": "^1.3.4",
		"@types/ioredis": "^4.27.6",
		"@types/jest": "^27.0.2",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/koa": "^2.13.4",
		"@types/koa-bodyparser": "^4.3.3",
		"@types/koa__cors": "^3.0.3",
		"@types/koa__router": "^8.0.8",
		"@types/node": "^16.10.2",
		"@types/ua-parser-js": "^0.7.36",
		"@types/ws": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"@yarnpkg/pnpify": "^3.0.1",
		"chokidar": "^3.5.2",
		"concurrently": "^6.3.0",
		"dotenv-cli": "^4.0.0",
		"eslint": "^7.32.0",
		"eslint-plugin-import": "^2.24.2",
		"jest": "^27.2.4",
		"node-dev": "^7.0.0",
		"pino-pretty": "^7.0.1",
		"prisma": "3.1.1",
		"source-map-support": "^0.5.20",
		"tslib": "^2.3.1",
		"typegraphql-prisma": "^0.16.1",
		"typescript": "^4.4.3"
	}
}
