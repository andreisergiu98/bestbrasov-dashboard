		"dotenv-cli": "^4.0.0",
		"eslint": "^7.28.0",
		"eslint-plugin-import": "^2.23.4",
		"prisma": "2.23.0",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.6",
