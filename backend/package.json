		"@types/eslint": "^7.2.13",
		"@types/graphql-fields": "^1.3.3",
		"@types/ioredis": "^4.26.4",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/koa": "^2.13.3",
		"@types/koa-bodyparser": "^4.3.1",
		"@types/koa-pino-logger": "^3.0.0",
		"@types/koa__cors": "^3.0.2",
		"@types/koa__router": "^8.0.4",
