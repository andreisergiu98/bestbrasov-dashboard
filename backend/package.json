{
	"name": "@bestbrasov/backend",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"scripts": {
		"prisma": "dotenv -- prisma",
		"prisma generate": "dotenv -- pnpify prisma generate",
		"lint": "eslint . --ext .js,.ts",
		"build": "rm -rf build && tsc && cp -r src/__generated__/prisma build/__generated__/prisma",
		"start:dev": "dotenv -- tsnd --inspect=0.0.0.0 --cache-directory=.cache --respawn --rs ./src/server.ts",
		"start:prod": "yarn node ./build/server.js",
		"start:emit-schema": "ts-node -r dotenv/config ./src/emit-schema"
	},
	"dependencies": {
		"@koa/cors": "^3.1.0",
		"@koa/router": "^10.0.0",
		"apollo-server-koa": "^2.25.1",
		"bullmq": "^1.34.0",
		"class-validator": "^0.13.1",
		"date-fns": "^2.22.1",
		"eventemitter3": "^4.0.7",
		"googleapis": "^76.0.0",
		"graphql": "^15.5.0",
		"graphql-fields": "^2.0.3",
		"graphql-redis-subscriptions": "^2.4.0",
		"graphql-scalars": "^1.10.0",
		"graphql-subscriptions": "^1.2.1",
		"ioredis": "^4.27.6",
		"jsonwebtoken": "^8.5.1",
		"koa": "^2.13.1",
		"koa-bodyparser": "^4.3.0",
		"koa-pino-logger": "^3.0.0",
		"module-alias": "^2.2.2",
		"openid-client": "^4.7.4",
		"pino": "^6.11.3",
		"pino-pretty": "^5.0.2",
