{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2020",
		"lib": ["es2020", "DOM"],
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": true,
		"noImplicitThis": true,
		"importHelpers": true,
		"alwaysStrict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	}
}
