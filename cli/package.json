{
	"name": "@bestbrasov/cli",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"bin": "./bin/run",
	"oclif": {
		"bin": "cli",
		"commands": "./lib/commands",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topics": {
			"backend": {
				"description": "backend application related operations"
			},
			"frontend": {
				"description": "frontend application related operations"
			},
			"db": {
				"description": "database related operations"
			},
			"db:migrate": {
				"description": "database migration related operations"
			}
		}
	},
	"dependencies": {
		"@oclif/command": "^1.8.0",
		"@oclif/config": "patch:@oclif/config@1.17.0#./patches/oclif-config-resolver.patch",
		"@oclif/errors": "^1.3.5",
		"@oclif/plugin-help": "^3.2.2",
		"chalk": "^4.1.1",
		"chokidar": "^3.5.2",
		"concurrently": "^6.2.0",
		"execa": "^5.1.1",
		"listr": "^0.14.3",
		"queue": "^6.0.2",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.26.0",
		"@types/concurrently": "^6.2.1",
		"@types/eslint": "^7.28.0",
		"@types/listr": "^0.14.4",
		"@types/node": "^16.3.3",
		"@yarnpkg/pnpify": "^3.0.0-rc.10",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5"
	},
	"files": [
		"/bin",
		"/lib",
		"/oclif.manifest.json"
	],
	"main": "lib/index.js",
	"scripts": {
		"build": "rm -rf lib && tsc -b && pnpify oclif-dev manifest",
		"update-readme": "pnpify oclif-dev readme",
		"prepack": "yarn build",
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint . --ext .ts --config .eslintrc",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	}
}
