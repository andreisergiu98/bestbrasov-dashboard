{
	"name": "@bestbrasov/cli",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"bin": "./bin/run",
	"oclif": {
		"bin": "yarn cli",
		"commands": "./src/commands",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topics": {
			"backend": {
				"description": "backend application related operations"
			},
			"frontend": {
				"description": "frontend application related operations"
			},
			"db": {
				"description": "database related operations"
			},
			"db:migrate": {
				"description": "database migration related operations"
			}
		}
	},
	"dependencies": {
		"@oclif/command": "^1.8.0",
		"@oclif/config": "patch:@oclif/config@1.17.0#./patches/oclif-config-resolver.patch",
		"@oclif/errors": "^1.3.4",
		"@oclif/plugin-help": "^3.2.2",
		"chokidar": "^3.5.1",
		"exec-sh": "^0.4.0",
		"queue": "^6.0.2",
		"shelljs": "^0.8.4",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.26.0",
		"@types/eslint": "^7.2.13",
		"@types/node": "^15.12.2",
		"@types/shelljs": "^0.8.8",
		"@yarnpkg/pnpify": "^3.0.0-rc.6",
		"ts-node": "^10.0.0",
		"typescript": "4.3.2"
	},
	"files": [
		"/bin",
		"/src",
		"/oclif.manifest.json"
	],
	"scripts": {
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint . --ext .ts --config .eslintrc",
		"prepack": "rm -rf lib && tsc -b && yarn pnpify oclif-dev manifest && pnpify yarn oclif-dev readme",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	},
	"main": "lib/index.ts",
	"types": "lib/index.d.ts"
}
