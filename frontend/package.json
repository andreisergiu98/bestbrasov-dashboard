{
	"name": "@bestbrasov/frontend",
	"version": "0.0.1",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "vite --host",
		"start:dev": "yarn generate && concurrently --raw  'yarn generate:dev' 'yarn start'",
		"build": "vite build && yarn post-build",
		"post-build": "cp vercel.json .dist",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"generate": "concurrently --raw 'yarn graphql-codegen' 'tsm .'",
		"generate:dev": "concurrently --raw 'yarn graphql-codegen -w' 'tsm . -w --ignoreInitial'"
	},
	"dependencies": {
		"@apollo/client": "^3.5.6",
		"@bestbrasov/common": "workspace:packages/common",
