		"@emotion/styled": "^11.3.0",
		"clsx": "^1.1.1",
		"eventemitter3": "^4.0.7",
		"framer-motion": "^4.1.17",
		"graphql": "^15.7.2",
		"graphql-ws": "^5.5.5",
		"history": "^5.1.0",
		"js-cookie": "^3.0.1",
		"react": "18.0.0-alpha-5cccacd13-20211101",
		"react-dom": "18.0.0-alpha-5cccacd13-20211101",
		"react-icons": "^4.3.1",
		"react-is": "18.0.0-alpha-5cccacd13-20211101",
		"react-router-dom": "^6.0.0",
