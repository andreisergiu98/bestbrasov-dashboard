		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"sass": "^1.43.4",
		"typed-scss-modules": "^4.1.1",
		"typescript": "^4.4.4",
		"vite": "^2.6.13",
		"vite-plugin-checker": "^0.3.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
