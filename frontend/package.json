		"eventemitter3": "^4.0.7",
		"framer-motion": "^4.1.17",
		"fuse.js": "^6.4.6",
		"graphql": "^15.8.0",
		"graphql-ws": "^5.5.5",
		"js-cookie": "^3.0.1",
