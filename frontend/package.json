		"@types/react-is": "^17.0.3",
		"@types/uniqid": "^5.3.2",
		"@types/yup": "^0.29.14",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"concurrently": "^7.2.2",
		"eslint": "^8.18.0",
